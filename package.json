{
  "name": "behavior-lens",
  "displayName": "Behavior Lens",
  "icon": "media/icon.png",
  "description": "Preview for BehaviorTree.CPP XML files",
  "publisher": "AntonioBrandi",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/AntoBrandi/BehaviorLens.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [
    "onCommand:behaviortree.preview",
    "onCommand:behaviortree.previewSide"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "behaviortree.preview",
        "title": "Open Preview",
        "category": "BehaviorLens"
      },
      {
        "command": "behaviortree.previewSide",
        "title": "Open Preview to the Side",
        "category": "BehaviorLens"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run build:webview",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build:webview": "vite build"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.75.0",
    "@types/xmldom": "^0.1.34",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "vite": "^5.0.0",
    "@vue-flow/background": "^1.3.0",
    "@vue-flow/controls": "^1.1.0",
    "@vue-flow/core": "^1.33.0",
    "dagre": "^0.8.5",
    "vue": "^3.4.0"
  },
  "dependencies": {
    "xmldom": "^0.6.0"
  }
}